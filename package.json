{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:web": "turbo run build --filter='./apps/*-web'",
    "build:elsa": "cd apps/elsa-designer && npm install && npm run build:web",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-custom": "@packages/workspace:*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0"
}
