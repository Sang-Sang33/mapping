{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:web": "turbo run build --filter='./apps/*-web'",
    "build:elsa": "cd apps/elsa-designer && npm install && npm run build:web",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "echo": "echo $1"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "turbo": "1.10.6"
  },
  "packageManager": "pnpm@7.15.0"
}
